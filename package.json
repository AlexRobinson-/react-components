{
  "name": "zd-react-components",
  "version": "3.1.0",
  "description": "Shared React components",
  "private": "true",
  "main": "./dist/react-components.js",
  "scripts": {
    "start": "styleguidist server",
    "build": "styleguidist build",
    "css-lint": "stylelint `find src -name '*.css'`",
    "jest": "jest --watch",
    "lint": "standard && npm run css-lint",
    "test": "jest && npm run lint",
    "dist": "webpack --progress && NODE_ENV=production webpack -p --progress",
    "changelog": "github-changes -o zendeskgarden -r react-components -a --only-pulls --use-commit-body",
    "gh-pages": "npm run build && gh-pages -d styleguide",
    "commit-dist": "git add dist && git commit -m 'Build dist'",
    "update-changelog": "npm run changelog && git add CHANGELOG.md && git commit -m 'Updated changelog'",
    "prepare-release": "./bin/is_git_clean && npm test && npm run dist && npm run commit-dist",
    "push-release": "git push --tags && npm run update-changelog && git push origin master:master",
    "patch": "npm run prepare-release && npm version patch && npm run push-release && npm run gh-pages",
    "minor": "npm run prepare-release && npm version minor && npm run push-release && npm run gh-pages",
    "major": "npm run prepare-release && npm version major && npm run push-release && npm run gh-pages",
    "travis": "npm test"
  },
  "keywords": [
    "react",
    "components"
  ],
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.14.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.9.2",
    "babel-standalone": "6.10.3",
    "chance-generators": "2.0.1",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.23.1",
    "file-loader": "0.9.0",
    "gh-pages": "0.11.0",
    "github-changes": "1.0.2",
    "identity-obj-proxy": "3.0.0",
    "isparta-loader": "2.0.0",
    "jest": "17.0.3",
    "json-loader": "0.5.4",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-jsdom-launcher": "3.0.0",
    "karma-mocha": "1.0.1",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.8.0",
    "mocha": "2.5.3",
    "postcss-cssnext": "^2.4",
    "postcss-import": "8.1.0",
    "postcss-input-range": "git+ssh://git@github.com/jonathantneal/postcss-input-range#ed1b254ba56316dbb4a939b86f39278bda53f4fa",
    "postcss-loader": "0.9.1",
    "react": "15.3.0",
    "react-addons-test-utils": "15.3.0",
    "react-dom": "15.3.0",
    "react-styleguidist": "4.3.3",
    "react-svg-loader": "1.0.1",
    "sinon": "1.17.4",
    "standard": "5.4.1",
    "stylelint": "^3.2.2",
    "stylelint-config-zendesk": "git+ssh://git@github.com/zendeskgarden/stylelint-config.git#1.0.2",
    "tabbable": "1.0.4",
    "unexpected": "10.20.0",
    "unexpected-check": "1.12.0",
    "unexpected-react": "3.3.2",
    "unexpected-sinon": "10.5.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.20",
    "zd-css-arrows": "git+ssh://git@github.com/zendeskgarden/css-arrows.git#0.3.0",
    "zd-css-avatars": "git+ssh://git@github.com/zendeskgarden/css-avatars.git#0.2.0",
    "zd-css-bedrock": "git+ssh://git@github.com/zendeskgarden/css-bedrock.git#0.4.0",
    "zd-css-buttons": "git+ssh://git@github.com/zendeskgarden/css-buttons.git#2.1.1",
    "zd-css-forms": "git+ssh://git@github.com/zendeskgarden/css-forms.git#0.3.1",
    "zd-css-menus": "git+ssh://git@github.com/zendeskgarden/css-menus.git#2.3.0",
    "zd-css-modals": "git+ssh://git@github.com/zendeskgarden/css-modals.git#2.1.0",
    "zd-css-tabs": "git+ssh://git@github.com/zendeskgarden/css-tabs.git#2.0.0",
    "zd-css-variables": "git+ssh://git@github.com/zendeskgarden/css-variables.git#0.9.0",
    "zd-svg-icons": "git+ssh://git@github.com/zendeskgarden/svg-icons.git#0.12.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "autosize": "3.0.17",
    "classnames": "2.2.5",
    "uuid": "2.0.2"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "afterEach"
    ],
    "ignore": "/dist/",
    "parser": "babel-eslint"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/test/styleMock.js",
      "test/expect": "<rootDir>/test/unexpected-with-plugins.js"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/index.js",
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules)/"
    ],
    "testRegex": "\\.*(spec\\.js)$"
  }
}
