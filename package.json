{
  "name": "zd-react-components",
  "version": "0.2.0",
  "description": "Shared React components",
  "private": "true",
  "main": "./dist/bundle.js",
  "scripts": {
    "start": "styleguidist server",
    "build": "styleguidist build",
    "lint": "standard && stylelint ./src/**/*.css",
    "test": "karma start --single-run; npm run lint",
    "dist": "webpack --progress && NODE_ENV=production webpack -p --progress",
    "changelog": "github-changes -o zendeskgarden -r react-components -a --only-pulls --use-commit-body",
    "gh-pages": "npm run build && gh-pages -d styleguide",
    "commit-dist": "git add dist && git commit -m 'Build dist'",
    "update-changelog": "npm run changelog && git add CHANGELOG.md && git commit -m 'Updated changelog'",
    "prepare-release": "./bin/is_git_clean && npm test && npm run dist && npm run commit-dist",
    "push-release": "git push --tags && npm run update-changelog && git push origin master:master",
    "patch": "npm run prepare-release && npm version patch && npm run push-release && npm run gh-pages",
    "minor": "npm run prepare-release && npm version minor && npm run push-release && npm run gh-pages",
    "major": "npm run prepare-release && npm version major && npm run push-release && npm run gh-pages",
    "travis": "npm test"
  },
  "keywords": [
    "react",
    "components"
  ],
  "devDependencies": {
    "babel-core": "6.9.1",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.9.2",
    "babel-standalone": "6.10.3",
    "chance-generators": "1.15.1",
    "css-loader": "0.23.1",
    "file-loader": "0.9.0",
    "gh-pages": "0.11.0",
    "github-changes": "1.0.2",
    "isparta-loader": "2.0.0",
    "json-loader": "0.5.4",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-jsdom-launcher": "3.0.0",
    "karma-mocha": "1.0.1",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.8.0",
    "mocha": "2.5.3",
    "postcss-cssnext": "^2.4",
    "postcss-import": "8.1.0",
    "postcss-loader": "0.9.1",
    "react": "15.3.0",
    "react-addons-test-utils": "15.3.0",
    "react-dom": "15.3.0",
    "react-styleguidist": "3.0.0",
    "react-svg-loader": "1.0.1",
    "sinon": "1.17.4",
    "stylelint": "^3.2.2",
    "stylelint-config-zendesk": "git+ssh://git@github.com/zendeskgarden/stylelint-config.git#1.0.2",
    "unexpected": "10.13.3",
    "unexpected-check": "1.7.0",
    "unexpected-react": "3.2.3",
    "unexpected-sinon": "10.2.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.4",
    "zd-css-arrows": "git+ssh://git@github.com/zendeskgarden/css-arrows.git#0.1.1",
    "zd-css-avatars": "git+ssh://git@github.com/zendeskgarden/css-avatars.git#0.2.0",
    "zd-css-bedrock": "git+ssh://git@github.com/zendeskgarden/css-bedrock.git#0.2.0",
    "zd-css-buttons": "git+ssh://git@github.com/zendeskgarden/css-buttons.git#1.1.0",
    "zd-css-forms": "git+ssh://git@github.com/zendeskgarden/css-forms.git#0.0.6",
    "zd-css-menus": "git+ssh://git@github.com/zendeskgarden/css-menus.git#1.2.0",
    "zd-css-modals": "git+ssh://git@github.com/zendeskgarden/css-modals.git#0.1.1",
    "zd-css-variables": "git+ssh://git@github.com/zendeskgarden/css-variables.git#0.3.0",
    "zd-svg-icons": "git+ssh://git@github.com/zendeskgarden/svg-icons.git#0.3.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "autosize": "3.0.17",
    "classnames": "2.2.5",
    "uuid": "2.0.2"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "afterEach"
    ],
    "ignore": "/dist/",
    "parser": "babel-eslint"
  }
}
