{
  "name": "@zendesk/garden-react-components",
  "version": "5.2.0",
  "description": "Shared React components",
  "repository": "zendeskgarden/react-components",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://zdrepo.jfrog.io/zdrepo/api/npm/npm"
  },
  "main": "./lib",
  "scripts": {
    "watch": "gulp watch",
    "start": "styleguidist server",
    "build": "styleguidist build",
    "prettier": "prettier --write --single-quote '{src,test}/**/*.js' '*.js'",
    "css-lint": "stylelint `find src -name '*.css'`",
    "jest": "jest --watch",
    "js-lint": "eslint '{src,test}/**/*.js'",
    "lint": "yarn js-lint && yarn css-lint",
    "test": "yarn lint && jest",
    "lib": "gulp",
    "changelog": "github-changes -o zendeskgarden -r react-components -a --only-pulls --use-commit-body",
    "gh-pages": "yarn build && gh-pages -d styleguide",
    "commit-lib": "git add lib && git commit -m 'Build lib'",
    "update-changelog": "yarn changelog && git add CHANGELOG.md && git commit -m 'Updated changelog'",
    "prepare-release": "yarn prettier && ./bin/is_git_clean && yarn test && yarn lib && yarn commit-lib",
    "push-release": "git push --tags && yarn update-changelog && git push origin master:master",
    "patch": "yarn prepare-release && npm version patch && yarn push-release && yarn gh-pages",
    "minor": "yarn prepare-release && npm version minor && yarn push-release && yarn gh-pages",
    "major": "yarn prepare-release && npm version major && yarn push-release && yarn gh-pages",
    "travis": "yarn prettier && yarn test && ./bin/is_git_clean"
  },
  "keywords": [
    "react",
    "components"
  ],
  "devDependencies": {
    "@zendesk/garden-css-arrows": "^1.0.0",
    "@zendesk/garden-css-avatars": "^1.0.1",
    "@zendesk/garden-css-bedrock": "^3.0.0",
    "@zendesk/garden-css-buttons": "^3.0.0",
    "@zendesk/garden-css-forms": "^2.1.0",
    "@zendesk/garden-css-labels": "^1.0.0",
    "@zendesk/garden-css-menus": "^3.0.0",
    "@zendesk/garden-css-modals": "^3.0.0",
    "@zendesk/garden-css-tabs": "^3.0.0",
    "@zendesk/garden-css-tooltips": "^2.0.0",
    "@zendesk/garden-css-variables": "^2.0.0",
    "@zendesk/garden-stylelint-config": "^5.0.0",
    "@zendesk/garden-svg-icons": "^2.1.0",
    "adler32": "0.1.7",
    "array-uniq": "1.0.3",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.23.0",
    "babel-standalone": "6.24.0",
    "chance-generators": "2.1.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-config-pretty-standard": "0.2.2",
    "file-loader": "0.11.1",
    "gh-pages": "0.12.0",
    "github-changes": "1.1.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-clean": "0.3.2",
    "gulp-clean-css": "3.0.4",
    "gulp-postcss": "6.4.0",
    "gulp-rename": "1.2.2",
    "gulp-transform": "1.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "19.0.2",
    "json-loader": "0.5.4",
    "postcss": "5.2.17",
    "postcss-cssnext": "2.10.0",
    "postcss-import": "8.1.0",
    "postcss-loader": "1.3.3",
    "postcss-modules": "0.6.4",
    "prettier": "1.2.2",
    "querystring": "0.2.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-styleguidist": "5.1.6",
    "react-svg-loader": "1.1.1",
    "sinon": "2.1.0",
    "stylelint": "7.10.1",
    "stylelint-order": "0.4.4",
    "sunesimonsen-postcss-input-range": "2.1.0",
    "unexpected": "10.27.0",
    "unexpected-check": "1.12.0",
    "unexpected-react": "3.5.2",
    "unexpected-sinon": "10.7.1",
    "url-loader": "0.5.8",
    "webpack": "2.4.1"
  },
  "dependencies": {
    "append-styles": "^1.0.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.5.8",
    "tabbable": "^1.0.6",
    "uuid": "^2.0.2 || ^3.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "files": [
    "lib"
  ],
  "standard": {
    "globals": [
      "afterEach",
      "beforeEach",
      "describe",
      "it",
      "jest"
    ],
    "ignore": "/lib/",
    "parser": "babel-eslint"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/test/styleMock.js",
      "test/expect": "<rootDir>/test/unexpected-with-plugins.js",
      "react-addons-test-utils": "react-dom/test-utils.js"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/index.js",
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules|lib)/"
    ],
    "testRegex": "\\.*(spec\\.js)$"
  }
}
