{
  "name": "react-theming",
  "description": "Zendesk Garden React Theming components",
  "author": "Zendesk Garden <garden@zendesk.com>",
  "license": "UNLISCENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/zendeskgarden/react-components"
  },
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "version": "0.0.5",
  "main": "./index.js",
  "scripts": {
    "build": "gulp --gulpfile ../../utils/build/gulpfile.build.js --cwd ./",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:js": "eslint -c '../../.eslintrc.json' './src/**/*.js'",
    "start": "styleguidist server --config ../../utils/styleguide/styleguide.base.config.js",
    "styleguide": "styleguidist build --config ../../utils/styleguide/styleguide.base.config.js",
    "prepublish": "gulp --gulpfile ../../utils/build/gulpfile.prepublish.js --cwd ./",
    "preversion": "echo 'testing'"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "styled-components": "^2.3.3"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16.0.0"
  }
}
